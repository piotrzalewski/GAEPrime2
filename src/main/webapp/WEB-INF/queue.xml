<queue-entries>
    <total-storage-limit>50M</total-storage-limit>
    <queue>
        <name>create</name>
        <rate>10/m</rate>
        <bucket-size>100</bucket-size>
        <max-concurrent-requests>10</max-concurrent-requests>
        <retry-parameters>
            <task-retry-limit>10</task-retry-limit>
            <task-age-limit>1d</task-age-limit>
            <min-backoff-seconds>720</min-backoff-seconds>
        </retry-parameters>
    </queue>
</queue-entries>